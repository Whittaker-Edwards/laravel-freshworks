{
    "name": "whittaker-edwards/laravel-freshworks",
    "description": "A Laravel wrapper for Freshworks API",
    "keywords": ["laravel", "freshworks", "freshsales", "crm", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Whittaker",
            "email": "ryan@whittakeredwards.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^6|^7|^8|^9|^10",
        "guzzlehttp/guzzle": "7.4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "WhittakerEdwards\\Freshworks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WhittakerEdwards\\Freshworks\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "WhittakerEdwards\\Freshworks\\FreshworksServiceProvider"
            ],
            "aliases": {
                "Freshworks": "WhittakerEdwards\\Freshworks\\FreshworksFacade"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
